#include <iostream>
using namespace std;
int main() {
    int a, b;
    cout << "Enter first number: ";
    cin >> a;
    cout << "Enter second number: ";
    cin >> b;
    a = a + b;
    b = a - b;
    a = a - b;
    cout << "After swapping:\n";
    cout << "First number: " << a;
    cout << "Second number: " << b;
    return 0;
}
